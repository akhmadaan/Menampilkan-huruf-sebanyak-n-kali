#include <iostream>

using namespace std;

int main()
{
    int n,a;
    cout<<"\nMenampilkan algoritma sebanyak n kali"<<endl;
    cout<<"masukan jumlah yang diinginkan :";
    cin>>n;
    cout<<" "<<endl;
    for(a=1;a<=n;++a)
    {
        if(a>10)
            break;
        cout<<"algoritma dan pemrograman"<<endl;
    }
        cout<<"\n hanya menampilkan 10 saja"<<endl;


    return 0;
}
